{
  "name": "backend-static-testing-setup",
  "version": "1.0.0",
  "description": "Static testing like eslint, prettier etc.",
  "scripts": {
    "build": "rm -rf ./dist && babel src --out-dir dist --ignore **/*.test.js",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"src**/*.+(js|json)\"",
    "format": "npm run lint -- --fix",
    "validate": "npm run lint && npm run testAndValidateBuild",
    "testAndValidateBuild": "npm run build",
    "start": "npm run build && node ./dist/index.js",
    "start:prod": "NODE_ENV=production npm run start",
    "start:local": "nodemon --config .nodemonrc.json"
  },
  "author": "jatin parate <jatin.parate@mlveda.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "@babel/core": "^7.15.4",
    "@babel/preset-env": "^7.15.4",
    "@babel/register": "^7.15.3",
    "axios": "^0.24.0",
    "core-js": "^3.17.2",
    "dotenv": "^10.0.0",
    "mongoose": "^5.7.12",
    "morgan": "^1.10.0",
    "regenerator-runtime": "^0.13.9",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.5.0"
  }
}
